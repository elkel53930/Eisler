defmodule TTLTO232(1:VCC 2:GND 3:TLLIN 4:TTLOUT 5:IN232 6:OUT232)
{
  wire TEST;

  part adm as ADM3202;
  part c as C;

  adm.VCC - _1.c4._2 - GND;
  adm.C1p - _1.c1._2 - C1m.adm;
  adm.C2p - _1.c2._2 - C2m.adm;
  adm.VCC - _1.c3._2 - Vp.adm;
  adm.Vm  - _1.c5._2 - GND;

  TTLIN - T1IN.adm;
  TTLOUT - R1OUT.adm;
  IN232 - R1IN.adm;
  OUT232 - T1OUT.adm;

  adm.VCC - VCC;
  adm.GND - GND;
  adm.T2IN - GND;
  adm.R2IN - GND;
  adm.T2OUT - nc;
  adm.R2OUT - nc;
}
